{
  "name" : "ipub/phone-ui",
  "type" : "library",
  "description" : "Phone extension template helpers & form validator for Nette Framework",
  "keywords" : [
    "nette",
    "phone",
    "tools",
    "ipub",
    "ipublikuj",
    "latte",
    "template"
  ],

  "homepage" : "https://github.com/ipublikuj-ui/phone",

  "license" : [
    "GPL-2.0",
    "GPL-3.0"
  ],

  "authors" : [
    {
      "name" : "iPublikuj:cms",
      "email" : "info@ipublikuj.eu",
      "homepage" : "http://www.ipublikuj.eu/"
    }
  ],

  "support" : {
    "email" : "support@ipublikuj.eu",
    "issues" : "https://github.com/ipublikuj-ui/phone/issues"
  },

  "extra" : {
    "ipub" : {
      "configuration" : {
        "extensions" : {
          "phoneUI" : "IPub\\PhoneUI\\DI\\PhoneUIExtension"
        }
      }
    }
  },

  "require" : {
    "php" : ">=7.2.0",

    "nette/application" : "^3.0",
    "nette/di" : "^3.0",
    "nette/utils" : "^3.0",
    "nette/forms" : "^3.0",

    "latte/latte" : "^2.5",

    "ipub/phone" : "^3.0"
  },

  "require-dev" : {
    "nette/bootstrap" : "^3.0",
    "nette/mail" : "^3.0",
    "nette/robot-loader" : "^3.0",
    "nette/safe-stream" : "^2.4",
    "nette/tester" : "^2.0",

    "tracy/tracy" : "^2.6",

    "pds/skeleton" : "~1.0"
  },

  "autoload" : {
    "psr-0" : {
      "IPub\\PhoneUI\\" : "src/"
    }
  }
}
